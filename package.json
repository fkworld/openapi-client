  "devDependencies": {
    "@antfu/eslint-config": "^5.1.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@ls-lint/ls-lint": "^2.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.31.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.4",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "turbo": "^2.5.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
