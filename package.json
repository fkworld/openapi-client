{
  "name": "openapi-clint",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": "22.16.0",
    "pnpm": "10.12.4"
  },
  "scripts": {
    "prepare": "husky",
    "lint:ls-lint": "ls-lint",
    "lint:eslint": "eslint --fix .",
    "lint:prettier": "prettier --write .",
    "lint:commitlint": "commitlint",
    "lint:lint-staged": "lint-staged",
    "clean": "pnpm dlx rimraf --glob '{node_modules,dist,.turbo}' 'packages/*/{node_modules,dist,.turbo}'"
